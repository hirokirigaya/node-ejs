<!DOCTYPE html>
<html>
  <%- include('../partial/head'); %>
  <body class="container">
    <%- include('../partial/header'); %>
    <main>
      <div class="jumbotron">
        <h1>Node com EJS</h1>
        <p>Vamos aprender a trabalhar com arquivos EJS</p>
        <p><%= subtitle %> </p>
      </div>
      <ul class="list-unstyled d-flex flex-column align-items-start display-6">
        <% qualitys.forEach(function(quality) { %>

          <li>
           <strong><%= quality.title %></strong> <%= quality.message %>
          </li>

       <% })%>
      </ul>
    </main>
  </body>
  <%- include('../partial/footer'); %>
</html>
